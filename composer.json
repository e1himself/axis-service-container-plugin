{
  "name": "axis/axis-service-container-plugin",
  "description": "symfony 1.x plugin that improves factories.yml config handler using Pimple and advanced configuring features",

  "type": "symfony1-plugin",
  "extra": {
    "installer-name": "AxisServiceContainerPlugin"
  },

  "keywords": ["symfony", "service container", "dependency injection", "factories", "pimple"],
  "license": "MIT",
  "homepage": "https://github.com/e1himself/axis-service-container-plugin",

  "authors": [
    {
      "name": "Ivan Voskoboynyk",
      "email": "ioann.voskoboynyk@gmail.com",
      "homepage": "https://github.com/e1himself"
    }
  ],
  
  "autoload": {
    "psr-0": {
      "Axis\\S1\\ServiceContainer": "lib/"
    }
  },

  "require": {
    "php": ">=5.3.0",
    "composer/installers": "*",
    "pimple/pimple": "1.*"
  }
}
